#include <stdio.h>

int main() 
{
    int i,odd,even;
    for(i=1;i<=10;i++)
    {
        printf("%d\n",i);
    }
    odd=0;
    for(i=1;i<=10;i=i+2)
    {
        odd=odd+i;
    }
    printf("odd sum = %d\n",odd);
    even=0;
    for(i=0;i<=10;i=i+2)
    {
        even=even+i;
    }
    printf("even sum = %d\n",even);
    return 0;
}
